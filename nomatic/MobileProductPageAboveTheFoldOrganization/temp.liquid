{%- assign setbundle = false -%}
{%- assign actual_price = blank -%}
{% if product.template_suffix == 'set-bundle' %}
  {% for tag in product.tags %}
    {% if tag contains 'bundle--' %}
      {%- assign setbundle = true -%}
      {%- assign tag_explore = tag | remove:'bundle--' -%}
	  {% capture setbundle_tags %}{{ setbundle_tags }},{{ tag_explore }}{% endcapture %}
    {% endif %}
  {% endfor %}
  {%- assign compare_price = 0 -%}
  {%- assign actual_price = 0 -%}
  {%- assign setbundle_tags = setbundle_tags | remove_first: ',' | split: ',' -%}
  {%- assign bundle_matrix = '' -%}
  {% for setbundle_tag in setbundle_tags %}
    {%- assign tag_split = setbundle_tag | split: '--' -%}
    {%- assign prod = all_products[tag_split[0]] -%}
	{%- assign prod_variant = prod.variants[0] -%}
	{%- assign prod_type = prod.type -%}
    {% if tag_split[1] != blank %}
	  {%- assign checkid = tag_split[1] | times: 1 -%}
	  {% for variant in prod.variants %}
		{% if variant.id == checkid %}
		  {%- assign prod_variant = variant -%}
          {% break %}
        {% endif %}
      {% endfor %}
    {% endif %}
	{% if prod_type == 'Bag' or prod_type == 'Luggage' %}
      {%- assign variant_price = prod_variant.price -%}
    {% else %}
	  {%- assign variant_price = prod_variant.price | divided_by: 2.00 -%}
    {% endif %}
    {%- assign actual_price = actual_price | plus: variant_price -%}
	{%- assign variant_normal_price = prod_variant.price -%}
	{% if prod_variant.compare_at_price != blank %}
      {%- assign variant_normal_price = prod_variant.compare_at_price -%}
    {% endif %}
<!-- {{ prod.title }} - {{ prod_variant.title }} -->
<!-- {{ variant_price }} -->
    {%- assign compare_price = compare_price | plus: variant_normal_price -%}
    {% capture bundle_matrix %}{{ bundle_matrix }}{ id: {{ prod_variant.id }}, quantity: 1, properties: {'BundleID': '{{ product.handle }}'} }{% unless forloop.last %},{% endunless %}{% endcapture %}
  {% endfor %}
  {%- assign actual_price_decimal = actual_price | split: '.' -%}
  {% if actual_price_decimal[1] != blank %}
    {%- assign actual_price = actual_price | plus: 1 -%}
  {% endif %}
  {%- assign compare_price_decimal = compare_price | split: '.' -%}
  {% if compare_price_decimal[1] != blank %}
    {%- assign compare_price = compare_price | plus: 1 -%}
  {% endif %}
{% endif %}


<section class='product-page'>
   {% include 'breadcrumbs' %}
   <div class="product-template__container container-fluid px-md-5" itemscope itemtype="http://schema.org/Product"
      id="ProductSection-{{ product.id }}" data-section-id="{{ product.id }}" data-section-type="product"
      data-enable-history-state="true" data-crossclick="{% if settings.enable_crossclick %}allow{% endif %}"
      data-comingsoon="{% if product.tags contains 'coming soon' %}true{% else %}false{% endif %}">
      {% include 'richcards' %}
      {% comment %}
      Get first variant, or deep linked one
      {% endcomment %}
      {%- assign current_variant = product.selected_or_first_available_variant -%}
      {%- assign product_thumb_size = '160x' -%}
      {%- assign product_image_zoom_size = '1024x1024' -%}
      {%- assign enable_zoom = section.settings.enable_zoom -%}
      {% case section.settings.image_size %}
      {% when 'small' %}
      {%- assign product_image_width = 'col-md-20' -%}
      {%- assign product_description_width = 'col-md-40' -%}
      {%- assign product_thumbnail_width = 'col-md-20' -%}
      {%- assign product_thumbnail_perrow = '3' %}
      {%- assign product_image_size = '345x' -%}
      {% when 'medium' %}
      {%- assign product_image_width = 'col-lg-30' -%}
      {%- assign product_description_width = 'col-lg-30' -%}
      {%- assign product_thumbnail_width = 'col-md-15' -%}
      {%- assign product_thumbnail_perrow = '4' %}
      {%- assign product_image_size = '530x' -%}
      {% when 'large' %}
      {%- assign product_image_width = 'col-md-40' -%}
      {%- assign product_description_width = 'col-md-20' -%}
      {%- assign product_thumbnail_width = 'col-md-12' -%}
      {%- assign product_thumbnail_perrow = '5' %}
      {%- assign product_image_size = '720x' -%}
      {% when 'full' %}
      {%- assign product_image_width = '' -%}
      {%- assign product_description_width = '' -%}
      {%- assign product_thumbnail_width = 'col-md-6' -%}
      {%- assign product_thumbnail_perrow = '10' %}
      {%- assign product_image_size = '1090x' -%}
      {%- assign enable_zoom = false -%}
      {% endcase %}
      {%- assign product_thumbnail_perrow = product_thumbnail_perrow | times: 1 -%}
      {%- assign prod_details = product.description | split: '<h6>' -%}
         {% assign current_variant = product.selected_or_first_available_variant %}
         {%- assign image_size = '540x600' -%}
         <div class="row product-single flex-md-row-reverse">
            <div class="{{ product_description_width }} product-description">
               <div class='product-detail-info'>
                  <div class='product-head'>
                     <div class='row'>
                        <div class='col-lg-30'>
                           {% if section.settings.show_vendor %}
                           <p>{{ product.vendor }}</p>
                           {% endif %}
                           <h2>{{ product.title }}</h2>
                           <div class='product-ratings'>
                              <div class="trustspot-inline-product" data-product-sku="{{ product.id }}" 
                              data-name="{{ product.title | escape }}"
                              data-url="{{ shop.url }}{{ product.url }}"
                              data-image-url="{{ product.featured_image | product_img_url: "large" |replace: '?', '%3F' | replace: '&','%26'}}"
                              data-description="{{ product.description | escape }}">
                           </div>
                        </div>
                     </div>
                     <div class='col-lg-30'>
                        <div class='product-head-right'>
                           <h2 class="product-single__price product-single__price-{{ product.id }}{% unless current_variant.available %} product-price--sold-out{% endunless %}">
                             {% if actual_price != blank %}
                                <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                                <s class="compareprice" id="ComparePrice-{{ product.id }}"><sup>{{currency.symbol}}</sup>{{ compare_price | money_with_currency }}
                                </s>
                                <span class="product-price__price product-price__price-{{ product.id }} product-price__sale product-price__sale--single">
                                <span id="ProductPrice-{{ product.id }}"><sup>{{currency.symbol}}</sup>{{ actual_price | money_with_currency}}</span></span>
                             {% else %}
                              {% if current_variant.compare_at_price > current_variant.price %}
                                <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                                <s class="compareprice" id="ComparePrice-{{ product.id }}"><sup>{{currency.symbol}}</sup>{{ current_variant.compare_at_price | money_with_currency }}
                                </s>
                                <span class="product-price__price product-price__price-{{ product.id }} product-price__sale product-price__sale--single">
                                <span id="ProductPrice-{{ product.id }}"><sup>{{currency.symbol}}</sup>{{ current_variant.price | money_with_currency}}</span>
                                <span class="product-price__sale-label product-price__sale-label-{{ product.id }}">{{ 'products.product.on_sale' | t }}</span>
                                </span>
                              {% else %}
                                <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                                  <s id="ComparePrice-{{ product.id }}"
                                     class="hide sale-price-item"><sup>{{currency.symbol}}</sup>{{ current_variant.compare_at_price | money_with_currency }}
                                  </s>
                                  <span class="product-price__price product-price__price-{{ product.id }}">
                                  <span id="ProductPrice-{{ product.id }}"><sup>{{currency.symbol}}</sup>{{ current_variant.price | money_with_currency}}</span>
                                  <span class="product-price__sale-label product-price__sale-label-{{ product.id }} hide ">{{ 'products.product.on_sale' | t }}</span>
                                </span>
                              {% endif %}
                             {% endif %}                                                                      
                           </h2>
                          {% if actual_price != blank %}
                          	{%- assign quadPrice = actual_price -%}
                          {% else %}                            
                          	{%- assign quadPrice = current_variant.price -%}                              
                          {% endif %} 
                           {% comment %} <!-- Insert QuadPay Widget --> {% endcomment %}
                          <div style="font-size: 14px; margin:10px 0 20px 0; min-height:20px;">
                            <quadpay-widget size="90%"
                                            logoSize="90%"
                                            alignment="right" 
                                            amountSelector="#ProductPrice-{{ product.id }}"  
                                            amount="{{ quadPrice | divided_by: 100.00 }}"
                                            breakpointAlignment="992 center">
                            </quadpay-widget>
                          </div>
                        </div>
                     </div>
                  </div>
               </div>
               {% for tag in product.tags %}
               {% if tag contains "f-" %}
               {% assign feature = true %}
               {% endif %}
               {% endfor %}
               {% if feature %}
               <div class='product-features d-lg-block d-none'>
                  {% capture features %}
                  <h2>Product Highlights</h2>
                  <ul>
                     {% for ftag in product.tags %}
                     {% if ftag contains "f-" %}
                     {% for block in section.blocks %}
                     {% if ftag contains block.settings.title %}
                     {%- assign tag = ftag | downcase | remove: "f-" -%}
                     <li> {% if block.settings.icon_image !=blank%} {% if block.settings.icon !=blank %}<em><img src='{{block.settings.icon | img_url: '200x'}}' alt='{{block.settings.icon.alt}}'/></em>
                        {% endif %}
                        {% else %}
                        <em class="fa {{ block.settings.name_icon }}"></em>
                        {% endif %}
                        {{tag}}
                     </li>
                     {% break %}
                     {% endif %}
                     {% endfor %}
                     {% endif %}
                     {% endfor %}
                  </ul>
                  {% endcapture %}
                  {{features}}
               </div>
               {% endif %}
               <div class='product-packs d-lg-block d-none'>
                  {% for tag in product.tags %}
                  {% if tag contains 'mindays' %}
                  {% assign currentmindays = tag | remove: 'mindays-' | plus: 0 %}
                  {% assign packs = true %}
                  {% endif %}
                  {% if tag contains 'maxdays' %}
                  {% assign currentmaxdays = tag | remove: 'maxdays-' | plus: 0 %}
                  {% endif %}
                  {% endfor %}
                  {% if packs %}
                  {% capture packsfor %}
                  <h2>Packs For</h2>
                  <ul class='packs-progress'>
                     {% for num in (1..7) %}
                     <li {% if num >= currentmindays and num <= currentmaxdays %} class="active"{% endif %}></li>
                     {% endfor %}
                  </ul>
                  <ul class='packs-labels'>
                     <li><a class="packs-additional-smaller" href="#">Need Smaller?</a></li>
                     <li><a href="#">{{currentmindays}}- {{currentmaxdays}} Days</a></li>
                     <li><a class="packs-additional-bigger" href="#">Need Bigger?</a></li>
                  </ul>
                  {% endcapture %}
                  {{packsfor}}
                  {% endif %}
                  {% capture smaller_bigger %}
                  <div class="packs-additional-product-smaller packs-additional-product product-items">
                     <div class="row">
                        {% for product in collections['all-products'].products %}
                        {% for tag in product.tags %}
                        {% if tag contains 'mindays' %}
                        {% assign mindays = tag | remove: 'mindays-' | plus: 0 %}
                        {% if mindays < currentmindays %}
                        <div class="col-md-20">{% include 'product-card-grid', grid_image_width: image_size %}</div>
                        {% endif %}
                        {% endif %}
                        {% endfor %}
                        {% endfor %}
                     </div>
                  </div>
                  <div class="packs-additional-product-bigger packs-additional-product product-items">
                     <div class="row">
                        {% for product in collections['all-products'].products %}
                        {% for tag in product.tags %}
                        {% if tag contains 'mindays' %}
                        {% assign mindays = tag | remove: 'mindays-' | plus: 0 %}
                        {% if mindays > currentmindays %}
                        <div class="col-md-20">
                           {% include 'product-card-grid', grid_image_width: image_size %}
                        </div>
                        {% endif %}
                        {% endif %}
                        {% endfor %}
                        {% endfor %}
                     </div>
                  </div>
                  {% endcapture %}
                  {{ smaller_bigger }}
               </div>
               <form action="/cart/add" method="post" enctype="multipart/form-data"
                  class="row align-items-end product-form product-form-{{ product.id }}{% unless section.settings.show_variant_labels %} product-form--hide-variant-labels{% endunless %}"
                  data-section="{{ product.id }}">
                  {% unless product.options.size == 1 and product.variants[0].title == 'Default Title' %}
                  {% for option in product.options_with_values %}
                  <div class="selector-wrapper {{ product.handle }} js col">
                     <label {% if option.name == 'default' %}class="label--hidden"
                     {% endif %}for="SingleOptionSelector-{{ forloop.index0 }}">
                     {{ option.name }}
                     </label>
                     <select class="single-option-selector single-option-selector-{{ product.id }} product-form__input"
                        id="SingleOptionSelector-{{ forloop.index0 }}"
                        data-index="option{{ forloop.index }}">
                     {% for value in option.values %}
                     <option value="{{ value | escape }}"{% if option.selected_value == value %} selected="selected"{% endif %}>{{ value }}</option>
                     {% endfor %}
                     </select>
                  </div>
                  {% if settings.enable_colorswatch %}
                  {% include 'swatch' with option.name %}
                  {% endif %}
                  {% endfor %}
                  {% endunless %}
                  <select name="id" id="ProductSelect-{{ product.id }}" data-section="{{ product.id }}"
                     class="product-form__variants col no-js">
                     {% for variant in product.variants %}
                     {% if variant.available %}
                     <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %}
                     value="{{ variant.id }}">
                     {{ variant.title }}
                     </option>
                     {% else %}
                     <option disabled="disabled">{{ variant.title }}
                        - {{ 'products.product.sold_out' | t }}
                     </option>
                     {% endif %}
                     {% endfor %}
                  </select>
                  
                 <div class="col product-form__item product-form__item--quantity{% unless section.settings.show_quantity_selector %} hide{% endunless %}">
                   <label for="Quantity">{{ 'products.product.quantity' | t }}</label>
                   <input type="number" id="Quantity" name="quantity" value="1" min="1"
                          class="product-form__input" pattern="[0-9]*">
                 </div>
                  <div class="col{% if settings.cartajax %}-60{% endif %} product-form__item product-form__item--submit">
                     <div class='product-btns'>
                        <div class='row'>
                           <div class='col-md-60 {% if product.tags contains 'bundle' %}col-xl-35{% else %}col-xl-60{% endif %} product-btn-mobile'>
                           <div class="button">
                              <div class="add">
                                 <button type="submit" name="add" id="AddToCart-{{ product.id }}"
                                 {% unless current_variant.available %}disabled="disabled"{% endunless %}
                                 class="btn product-form__cart-submit add-to-cart">
                                 <span id="AddToCartText-{{ product.id }}">
                                 {% unless current_variant.available %}
                                 {{ 'products.product.sold_out' | t }}
                                 {% else %}
                                 {% if product.tags contains 'coming soon' %}
                                 {{ 'products.product.coming_soon' | t }}
                                 {% else %}
                                 {{ 'products.product.add_to_cart' | t }}
                                 {% endif %}
                                 {% endunless %}
                                 </span>
                                 </button>

                                 {% if settings.cartajax %}
                                 <a class="btn checkout" href="/cart"><i class="fa fa-shopping-cart"
                                    aria-hidden="true"></i><span>Checkout</span></a>
                                 <a class="btn cont"
                                    href="#">
                                 <i class="fa fa-angle-right continue-arrow" aria-hidden="true"></i><span>Continue Shopping</span></a>
                                 {% endif %}
                              </div>
                           </div>
                        </div>

                        {% comment %}{% assign current_product = product.handle %}
                        {% for product in collections.all-products.products %}
                        {% for tag in product.tags %}
                        {% if tag contains current_product %} 
                        {% endif %}
                        {% endfor %}
                        {% endfor %} {% endcomment %}
                        {% if product.tags contains 'bundle' or product.tags  contains 'Bundle' %}
                        <div class='col-md-60 col-xl-25 pl-3 pl-xl-0'>
                           <a class="btn pro-btn" href="#upsell_btns" varient="{{ product.variants[0].id }}">Bundle & Save</span></a>
                           {% comment %} 
                           <p class="pro-btn-note">Adds a shirt organizer, toiletry bag, compression cube & vacuum bag</p>
                           {% endcomment %}
                        </div>
                        {% endif %}
                     </div>
                  </div>
            </div>
            </form>
              
            {% capture accordian %}

            <div class="product-description d-lg-block d-block">
               <div class="accordion">
                  {% for prod_detail in prod_details %}
                  {% if forloop.index >=2 and forloop.index %}
                  {%- assign prod_parts = prod_detail | split: '</h6>' -%}
                  {% if prod_parts[0] == "Split" %}
                  {% continue %}
                  {% endif %}
                  <div class="card mb-1">
                  <div class="card-header accordion_head">
                  <h2 class="mb-0">
                  <button type="button" class="btn btn-link accordion_head">{{ prod_parts[0] }}<span class="plusminus"><em class="fa fa-plus"></em></span></button>
                  </h2>
                  </div>
                  <div class="accordion_body card-body" style="display: none;">
                  {{ prod_parts[1] }}
                  </div>
                  </div>
                  {% endif %}
                  {% endfor %}
                  </div>
                  <div class="product-social">
                  {% if section.settings.show_share_buttons %}
                  {% include 'social-sharing', share_title: product.title, share_permalink: product.url, share_image: product %}
                  {% endif %}
                  </div>
                  </div>
                  {% endcapture %}
                  <div class="d-lg-block d-none">
                  {{ accordian }}
                  </div>
                  </div>
   </div>
   <div class="product-single__photos {{ product_image_width }}{% if section.settings.image_size == 'full' %} product-single__photos--full{% endif %}">
      {%- assign featured_img_src = current_variant.featured_image.src | default: product.featured_image.src -%}
      {%- assign featured_img_alt = current_variant.featured_image.alt | default: product.featured_image.alt -%}
      {% if settings.enable_crossclick %}
      {%- assign prod_image_size = 0 -%}
      {% for image in product.images %}
      {% if image.attached_to_variant? %}
      {%- assign prod_image_size = prod_image_size | plus: 1 -%}
      {% endif %}
      {% endfor %}
      {% else %}
      {%- assign prod_image_size = product.images.size -%}
      {% endif %}
      {% if section.settings.vertical and prod_image_size > 1 %}
      <div class="row vertical-thumbs">
         <div class="col-lg-50 push-lg-10 product-videos">
            {% endif %}
            <div id="FeaturedImageZoom-{{ product.id }}"
            class="d-none d-lg-block product-single__photo{% if product.images.size > 1 %} product-single__photo--has-thumbnails{% endif %}"
            {% if enable_zoom %}data-zoom="{{ featured_img_src | img_url: product_image_zoom_size }}"{% endif %}
            >
            <img src="{{ featured_img_src | img_url: product_image_size }}" alt="{{ featured_img_alt }}"
               id="FeaturedImage-{{ product.id }}"
               class="product-featured-img{% if enable_zoom %} js-zoom-enabled{% endif %}">
            {% assign video = false %}
            {% for image in product.images %}
            {% if image.alt contains 'youtube' %}
            {% assign video_id = image.alt | split: '-' %}
            {% assign video = true %}
            {% assign video_url = "http://www.youtube.com/watch?v=" | append: video_id[1] %}
            {% elsif image.alt contains 'vimeo' %}
            {% assign video_id = image.alt | split: '-' %}
            {% assign video = true %}
            {% assign video_url = "https://vimeo.com/" | append: video_id[1] %}
            {% endif %}
            {% endfor %}
            {% if video %}
            <a class="popup-video product-video-icon" href="{{video_url}}" style="display:none"><em class="far fa-play-circle"></em></a>
            {% endif %}
         </div>
         {% comment %} Mobile product image slider {% endcomment %}
         <div class="product-image-mobile d-block d-lg-none">
            <div class="slidersmobile ">
               {% if enable_buttons == true %}
               <a id="prevArrow" class="slick-prev">
               <i class="fa fa-angle-{% if section.settings.vertical %}up{% else %}left{% endif %}"></i>
               </a>{% endif %}
               <ul class="sliderrel">
                  {% if settings.enable_crossclick %}
                  {%- assign attach_tot = 0 -%}
                  {%- assign cross_need = false -%}
                  {% for image in product.images %}
                  {% if image.attached_to_variant? %}
                  {%- assign attach_tot = attach_tot | plus: 1 -%}
                  {% include 'loop-thumbnail' %}
                  {% endif %}
                  {% endfor %}
                  {% unless attach_tot == product.images.size %}
                  {%- assign cross_need = true -%}
                  {% endunless %}
                  {% else %}
                  {% for image in product.images %}
                  <div class="product-single__photo">
                     <img src="{{ image | img_url: product_image_size }}" alt="" class="product-featured-img">
                     {% assign video = false %}
                     {% if image.alt contains 'youtube' %}
                     {% assign video_id = image.alt | split: '-' %}
                     {% assign video = true %}
                     {% assign video_url = "http://www.youtube.com/watch?v=" | append: video_id[1] %}
                     {% elsif image.alt contains 'vimeo' %}
                     {% assign video_id = image.alt | split: '-' %}
                     {% assign video = true %}
                     {% assign video_url = "https://vimeo.com/" | append: video_id[1] %}
                     {% endif %}
                     {% if video %}
                     <a class="popup-video product-video-icon" href="{{video_url}}"><em class="far fa-play-circle"></em></a>
                     {% endif %}
                  </div>
                  {% endfor %}
                  {% endif %}
               </ul>
               {% if enable_buttons == true %}
               <a id="nextArrow" class="slick-next">
               <i class="fa fa-angle-{% if section.settings.vertical %}down{% else %}right{% endif %}"></i>
               </a>{% endif %}
            </div>
            <div class="slidersmobile">
               {% if enable_buttons == true %}
               <a id="prevArrow" class="slick-prev">
               <i class="fa fa-angle-{% if section.settings.vertical %}up{% else %}left{% endif %}"></i>
               </a>{% endif %}
               <ul class="slider-nav">
                  {% if settings.enable_crossclick %}
                  {%- assign attach_tot = 0 -%}
                  {%- assign cross_need = false -%}
                  {% for image in product.images %}
                  {% if image.attached_to_variant? %}
                  {%- assign attach_tot = attach_tot | plus: 1 -%}
                  {% include 'loop-thumbnail' %}
                  {% endif %}
                  {% endfor %}
                  {% unless attach_tot == product.images.size %}
                  {%- assign cross_need = true -%}
                  {% endunless %}
                  {% else %}
                  {% for image in product.images %}
                  {% include 'loop-thumbnail' %}
                  {% endfor %}
                  {% endif %}
               </ul>
               {% if enable_buttons == true %}
               <a id="nextArrow" class="slick-next">
               <i class="fa fa-angle-{% if section.settings.vertical %}down{% else %}right{% endif %}"></i>
               </a>{% endif %}
            </div>
         </div>
         {% comment %} Product image slider ends here {% endcomment %}
         {% if prod_image_size > product_thumbnail_perrow %}
         {%- assign enable_buttons = true -%}
         {% endif %}
         {% if prod_image_size > 1 %}
         {% if section.settings.vertical and prod_image_size > 1 %}
      </div>
      <div class="col-lg-10 pull-lg-50 d-none d-lg-block">
         {% endif %}
         <div class="thumbnails-wrapper d-none d-lg-block">
            {% if enable_buttons == true %}
            <a id="prevArrow" class="slick-prev">
            <i class="fa fa-angle-{% if section.settings.vertical %}up{% else %}left{% endif %}"></i>
            </a>{% endif %}
            <ul class="product-single__thumbnails desktop-only product-single__thumbnails-{{ product.id }} ">
               {% if settings.enable_crossclick %}
               {%- assign attach_tot = 0 -%}
               {%- assign cross_need = false -%}
               {% for image in product.images %}
               {% if image.attached_to_variant? %}
               {%- assign attach_tot = attach_tot | plus: 1 -%}
               {% include 'loop-thumbnail' %}
               {% endif %}
               {% endfor %}
               {% unless attach_tot == product.images.size %}
               {%- assign cross_need = true -%}
               {% endunless %}
               {% else %}
               {% for image in product.images %}
               {% include 'loop-thumbnail' %}
               {% endfor %}
               {% endif %}
            </ul>
            {% if enable_buttons == true %}
            <a id="nextArrow" class="slick-next">
            <i class="fa fa-angle-{% if section.settings.vertical %}down{% else %}right{% endif %}"></i>
            </a>{% endif %}
         </div>
         {% endif %}
         {% if section.settings.vertical and prod_image_size > 1 %}
      </div>
   </div>
   {% endif %}
   </div>
   <div class="col-60 d-lg-none d-block">
      <div class="product-features">
         {{features}}
      </div>
      <div class='product-packs'>
         {{packsfor}}
         {{ smaller_bigger }}
      </div>
      {{ accordian }}
   </div>
   </div>
   </div>
