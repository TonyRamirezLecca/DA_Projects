<script>
   $(document).ready(function () {
       $(window).on("resize", function (e) {
           checkScreenSize();
       });
       checkScreenSize();
       function checkScreenSize(){
           var newWindowWidth = $(window).width();
           if (newWindowWidth < 576) {
               $(window).scroll(function() {
                   if ($(this).scrollTop()>400)
                   {
                       $('.product-btn-mobile').addClass('sticky');
                   }
                   else
                   {
                   $('.product-btn-mobile').removeClass('sticky');
                   }
               });
           }
         }
   });
</script>
<script>
   $(document).ready(function(){
     $(".product-single__thumbnails.desktop-only .product-single__thumbnail").click(function(){
         $(".product-single__photo .product-video-icon").fadeOut('fast');
         if($('.product-single__photo .product-featured-img').hasClass('js-zoom-enabled')==false) {
            $('.product-single__photo .product-featured-img').addClass('js-zoom-enabled');
            $('.zoomImg').removeClass('d-none');
         }
         if($(this).closest('.product-single__thumbnails-item').hasClass('video-thumb')) {
               setTimeout(function() {
                  $('.zoomImg').remove();
               },500)
         }
     });
     $(".video-thumb").click(function(){
      $(".product-single__photo .product-video-icon").fadeIn('slow');
      $('.product-single__photo .product-featured-img').removeClass('js-zoom-enabled');
      $('.zoomImg').addClass('d-none');
     });
     $('.popup-video').magnificPopup({
       type: 'iframe',
       mainClass: 'mfp-fade',
       removalDelay: 160,
       preloader: false,
       fixedContentPos: false
     });
     
   });
</script>
